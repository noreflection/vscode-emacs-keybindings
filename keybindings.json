[
  // editor
  {
    "key": "ctrl+q",
    "command": "workbench.action.quickOpenView" //open list with pannels
  },
  {
    "key": "alt+s",
    "command": "workbench.view.explorer"
  },
  {
    "key": "alt+e",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  // navigation
  {
    "key": "alt+z",
    "command": "workbench.action.openPreviousRecentlyUsedEditorInGroup"
  },
  // commands
  {
    "key": "alt+enter",
    "command": "editor.action.quickFix",
    "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+c",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
  },
  // plugins
  {
    "key": "ctrl+alt+m",
    "command": "code-runner.run"
  },
  //emacs
  {
    "key": "ctrl+x left",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "ctrl+x right",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "ctrl+k",
    "command": "emacs.C-S_bs"
  },
  {
    "key": "ctrl+o ctrl+f",
    "command": "workbench.action.files.openFolder"
  },
  {
    "key": "ctrl+.",
    "command": "breadcrumbs.focusAndSelect",
    "when": "breadcrumbsVisible"
  },
  {
    "key": "ctrl+shift+.",
    "command": "-breadcrumbs.focusAndSelect",
    "when": "breadcrumbsVisible"
  },
  {
    "key": "alt+g",
    "command": "workbench.view.scm"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.scm"
  },
  {
    "key": "alt+1",
    "command": "-workbench.action.openEditorAtIndex1"
  },
  {
    "key": "alt+f12",
    "command": "-editor.action.previewDeclaration",
    "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor"
  },
  {
    "key": "alt+q",
    "command": "editor.action.previewDeclaration"
  },
  // expand select region
  {
    "key": "ctrl+w","command": "expand_region", "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+w","command": "undo_expand_region", "when": "editorTextFocus && editorHasSelection"
  }
]
